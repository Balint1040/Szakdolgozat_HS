generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model imageurl {
  id        Int    @id @default(autoincrement())
  url       String
  productId Int

  @@index([productId], map: "ImageUrl_productId_fkey")
}

model product {
  id           Int    @id @default(autoincrement())
  name         String
  price        Int
  properties   Json
  manufacturer String
  categoryId   Int    @default(1)
}
