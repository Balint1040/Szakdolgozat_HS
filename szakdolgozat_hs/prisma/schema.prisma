generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Product {
  id          Int      @id @default(autoincrement())
  name        String
  price       Int
  properties  Json
  manufacturer String
  //categoryId  Int @default(4)
  imageUrls   ImageUrl[]

}

model ImageUrl {
  id Int @id @default(autoincrement())
  url String
  productId Int
  product Product @relation(fields: [productId], references: [id])
}
